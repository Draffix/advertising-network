<body class="login">
<div id="content">
	<div class="container-fluid">
		<div class="lock-container">
			<h1>Přihlášení k účtu</h1>

			<div class="panel panel-default text-center">
				<img src="images/people/110/guy-5.jpg" class="img-circle">

				<div class="panel-body">
					<form action="/signin" method="post">
						<input class="form-control" type="text" name="username" placeholder="Uživatelské jméno">
						<input class="form-control" type="password" name="password" placeholder="Heslo">
						<button type="submit" class="btn btn-primary">Přihlásit se<i class="fa fa-fw fa-unlock-alt"></i></button>
					</form>
					<br>
					<a href="/oauth/facebook">Přihlásit se pomocí účtu Facebook</a><br>
					<a href="/oauth/twitter">Přihlásit se pomocí účtu Twitter</a><br>
					<a href="/oauth/google">Přihlásit se pomocí účtu Google</a><br>
					<!--<a href="#" class="forgot-password">Zapomněli jste heslo?</a>-->
					<% for(var i in messages) { %>
					<div class="flash"><%= messages[i] %></div>
					<% } %>
				</div>
			</div>
		</div>
	</div>
</div>

